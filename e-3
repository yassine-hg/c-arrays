#include <stdio.h>

int main(){
    int n ;
    int t[100];
    int sum = 0;

    printf("Input the number of elements to be stored in the array: ");
    scanf("%d", &n);

    for(int i = 0 ; i < n ; i++ ){
        printf("Element - %d : ", i);
        scanf("%d", &t[i]);
        sum += t[i];
    }
    printf("Sum of all elements stored in the array is: %d", sum);

}
