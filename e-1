#include <stdio.h>

int main(){
    int t[10];

    for (int i = 0 ; i < 10 ; i ++){
        printf("element - %d: ", i);
        scanf("%d", &t[i]);
    }
    printf("\n elemnt in the array are: ");
    for (int i = 0;i <10; i++){
        printf("%d\t", t[i]);
    }
    printf("\n");
    return 0;
}
